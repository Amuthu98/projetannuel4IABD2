    "def winner(score):\n",
    "    string=str(score)\n",
    "    chaine=(\"'winner':\")\n",
    "    long=len(chaine)\n",
    "    result=string.find(chaine)\n",
    "    if result>-1:\n",
    "        debut=result+long\n",
    "        i=0\n",
    "        winner=\"\"\n",
    "        while i==0:\n",
    "            if string[debut]==',':\n",
    "                i=1\n",
    "                break\n",
    "            winner=winner+string[debut]\n",
    "            debut+=1\n",
    "    if (winner==\" 'DRAW'\"):\n",
    "        winner=0            \n",
    "    if (winner==\" 'HOME_TEAM'\"):\n",
    "        winner=1\n",
    "    if (winner==\" 'AWAY_TEAM'\"):\n",
    "        winner=2\n",
    "    if (winner==\" None\"):\n",
    "        winner=None\n",
    "    \n",
    "    return winner"
